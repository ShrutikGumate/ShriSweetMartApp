<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sweetshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo2_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AboutUs"></activity>
        <activity android:name=".ContactUs" />
        <activity android:name=".Feedback" />
        <activity android:name=".UserProfile" />
        <activity android:name=".OrderHistory" />
        <activity
            android:name=".forget_password"
            android:parentActivityName=".Login" />
        <activity android:name=".OTP" />
        <activity android:name=".Introductory">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CatTwoDetails"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CatOneDetails"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Login" />
        <activity android:name=".SignUp" />
    </application>

</manifest>